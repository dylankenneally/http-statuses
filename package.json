    "@netlify/plugin-lighthouse": "^6.0.4",
    "dotenv": "^17.3.1",
    "eleventy-plugin-gen-favicons": "^1.1.3",
    "html-minifier": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "rimraf": "^6.1.2"
  },
  "dependencies": {
    "axios": "^1.13.5"
