    "@netlify/plugin-lighthouse": "^6.0.1",
    "dotenv": "^16.5.0",
    "eleventy-plugin-gen-favicons": "^1.1.3",
    "html-minifier": "^4.0.0",
    "rimraf": "^6.0.1"
